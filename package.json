    "eslint": "^8.9.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.68.0",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "v8-compile-cache": "^2.3.0"
  },
  "dependencies": {
    "react-grid-layout": "^1.3.4",
    "react-grid-system": "^8.1.1"
